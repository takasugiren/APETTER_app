<!--自分の投稿に対するいいね、コメントは通知しない-->
<% if notifications = @notifications.where.not(visitor_id: current_user.id) %>
  <% if notifications.exists? %>
    <%= render @notifications %>
  <% else %>
  <div class="container">
    <div class="row justify-content-center mt-4">
      <div class="col-12 col-lg-10">
        <div class="white-back">
          <div class="col-12">
            <%= link_to "", tweets_path, class: "browser-back far fa-arrow-alt-circle-left", style: "text-decoration: none; color: #444444; margin-top: 10px" %>
          </div>
          <div class="nomember-back">
            <p class="nomember">お知らせがありません。</p>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% end %>
<% end %>