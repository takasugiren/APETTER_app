<% if user_signed_in? %>
  <%= form_with url: search_path, class: "form-inline", local: true, method: :get do |f| %>
      <%= f.select :range, options_for_select([['ユーザー'], ['ツイート'], ['タグ']]), {}, {class: "select-tag"} %>
      <%= f.text_field :word, id: "search-text",  placeholder: "キーワードを入力" %>
  <% end %>
<% end %>